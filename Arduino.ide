#include<SoftwareSerial.h>

#define rxPin 10
#define txPin 11

SoftwareSerial mySerial(rxPin, txPin);

int sensorPin = A0;

int sensorValue = 0;

void setup()
{
  mySerial.begin(9600);
  while (!mySerial)
  {  }
}

void loop()
{
  sensorValue = analogRead(sensorPin);
  mySerial.write(sensorValue/3);
  delay(100); // freqency : 1000/delay
}
